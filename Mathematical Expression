#include<stdio.h>
#include<math.h>
int main ()
{
    long long a, b, c, result;
    char s, q;
    scanf("%lld %c %lld %c %lld", &a, &s, &b, &q, &c);
    
    if(s== '+')
    {
        result=a+b;
    }
    else if(s== '-')
    {
        result=a-b;
    }
    else if(s== '*')
    {
        result=a*b;
    }
    if(result==c)
    {
        printf("Yes\n");
    }
    else
    {
        printf("%d\n", result);
    }
 
    return 0;
}
